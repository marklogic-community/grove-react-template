{
  "name": "muir-react-template",
  "version": "0.10.0-alpha.3",
  "description": "Turning MarkLogic forests into a place for users to live",
  "devDependencies": {
    "concurrently": "^3.6.1"
  },
  "dependencies": {},
  "scripts": {
    "build": "cd ui && npm run build",
    "lint": "cd middle-tier && npm run lint; cd ../ui && npm run lint",
    "lint:fix": "cd middle-tier && npm run lint:fix; cd ../ui && npm run lint:fix",
    "postinstall": "node scripts/postinstall.js",
    "start": "concurrently --kill-others \"npm run middleTier\" \"npm run ui\" ",
    "start:prod": "cd middle-tier && npm run start:prod",
    "middleTier": "cd middle-tier && npm start",
    "ui": "cd ui && npm start",
    "test": "concurrently 'cd middle-tier && npm test' 'cd ui && npm test'"
  },
  "repository": {
    "type": "git",
    "url": "ssh://project.marklogic.com:7999/nacw/muir.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Patrick McElwee",
      "email": "patrick.mcelwee@marklogic.com"
    },
    {
      "name": "Frank Sanders",
      "email": "frank.sanders@marklogic.com"
    }
  ]
}
