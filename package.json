{
  "name": "muir-react-template",
  "version": "0.7.0",
  "description": "Turning MarkLogic forests into a place for users to live",
  "dependencies": {
    "concurrently": "^3.5.0"
  },
  "devDependencies": {},
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "start": "concurrently --kill-others \"npm run server\" \"npm run ui\" ",
    "start:prod": "concurrently \"npm run server:prod\" \"npm run ui\" ",
    "server": "cd server && npm start",
    "server:prod": "cd server && npm run start:prod",
    "ui": "cd ui && npm start",
    "test": "cd ui && npm test"
  },
  "repository": {
    "type": "git",
    "url": "ssh://project.marklogic.com:7999/nacw/muir.git"
  },
